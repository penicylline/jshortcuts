{
  "manifest_version": 2,

  "name": "JShortcuts",
  "description": "Shortcuts to execute javascript.",
  "version": "1.0.0",
  "icons": {
      "16": "images/jshotcuts.png",
      "48": "images/jshotcuts.png",
      "128": "images/jshotcuts.png" 
  },
  "browser_action": {
    "default_icon": "images/jshotcuts.png",
    "default_popup": "main.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
